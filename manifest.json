{
    "manifest_version": 2,
    "name": "Switch Tabs",
    "version": "1.2.0",
    "description": "Switch tabs every x seconds.",
    "background": {
        "scripts": ["./js/background.js"]
    },
    "browser_action": {
        "default_icon": "./img/icon-disabled.png",
        "default_popup": "popup.html"
    },
    "permissions": ["tabs", "storage"]
}
